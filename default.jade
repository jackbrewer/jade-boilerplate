doctype
html.no-js(lang='en')
  head
    include includes/document-head

  body

    //- Ensure correct analytic properties are set before uncommenting.
    //- include includes/analytics

    block body-start

    block main-header
      include includes/main-header

    block main-navigation
      include includes/main-navigation

    main.main-content(role='main')
      block content

    block main-footer
      include includes/main-footer

    block body-end

    //- Scripts
    script(src='//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js')
    script.
      window.jQuery || document.write('<script src="/static/js/lib/vendor/jquery-1.11.0.min.js"><\/script>')

    block scripts
      script(src=versionPath('/static/js/lib/build/index.js'))
